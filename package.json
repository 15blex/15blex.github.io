{
  "name": "15blex.github.io",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "npm run build && npx gh-pages -d build -t",
    "dev": "vite dev",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "oxlint src --type-aware",
    "lint:fix": "oxlint src --fix --type-aware",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.10.1",
    "@wuchale/svelte": "^0.17.5",
    "@wuchale/vite-plugin": "^0.15.3",
    "gh-pages": "^6.3.0",
    "oxlint": "^1.29.0",
    "oxlint-tsgolint": "^0.8.1",
    "prettier": "^3.6.2",
    "svelte": "^5.43.14",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "wuchale": "^0.18.5"
  }
}
